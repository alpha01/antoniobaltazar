{
  "environment": {
    "APP_HOST": "webportfolio.marathon.l4lb.thisdcos.directory",
    "APP_ENV": "prod"
  },
  "id": "/web/portfolio",
  "version": "2020-03-02T05:55:11.736Z",
  "containers": [
    {
      "name": "app",
      "resources": {
        "cpus": 0.1,
        "mem": 512,
        "disk": 0,
        "gpus": 0
      },
      "image": {
        "kind": "DOCKER",
        "id": "registry.hub.docker.com/alpha01jenkins/portfolio_app:##TAG##",
        "forcePull": true
      },
      "healthCheck": {
        "http": {
          "scheme": "HTTP",
          "endpoint": "app"
        },
        "gracePeriodSeconds": 300,
        "intervalSeconds": 60,
        "maxConsecutiveFailures": 3,
        "timeoutSeconds": 20,
        "delaySeconds": 15
      },
      "endpoints": [
        {
          "name": "app",
          "containerPort": 80,
          "hostPort": 0,
          "protocol": [
            "tcp"
          ],
          "labels": {
            "VIP_0": "web/portfolio:80"
          }
        }
      ]
    },
    {
      "name": "varnish",
      "resources": {
        "cpus": 0.3,
        "mem": 1024,
        "disk": 0,
        "gpus": 0
      },
      "image": {
        "kind": "DOCKER",
        "id": "registry.hub.docker.com/alpha01jenkins/portfolio_varnish:##TAG##",
        "forcePull": true
      },
      "healthCheck": {
        "tcp": {
          "endpoint": "varnish"
        },
        "gracePeriodSeconds": 300,
        "intervalSeconds": 60,
        "maxConsecutiveFailures": 3,
        "timeoutSeconds": 20,
        "delaySeconds": 15
      },
      "endpoints": [
        {
          "name": "varnish",
          "containerPort": 6081,
          "hostPort": 6081,
          "protocol": [
            "tcp"
          ]
        }
      ]
    }
  ],
  "networks": [
    {
      "name": "dcos",
      "mode": "container"
    }
  ],
  "scaling": {
    "instances": 2,
    "kind": "fixed"
  },
  "scheduling": {
    "backoff": {
      "backoff": 10,
      "backoffFactor": 1.15,
      "maxLaunchDelay": 300
    },
    "upgrade": {
      "minimumHealthCapacity": 1,
      "maximumOverCapacity": 1
    },
    "placement": {
      "constraints": [
        {
          "fieldName": "hostname",
          "operator": "UNIQUE"
        }
      ]
    },
    "killSelection": "YOUNGEST_FIRST",
    "unreachableStrategy": {
      "inactiveAfterSeconds": 10,
      "expungeAfterSeconds": 20
    }
  },
  "executorResources": {
    "cpus": 0.1,
    "mem": 32,
    "disk": 10
  },
  "role": "web",
  "volumes": [],
  "fetch": []
}